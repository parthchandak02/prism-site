{
  "name": "nextjs-prism",
  "version": "1.0.0",
  "description": "An interactive 3D prism visualization built with React and Three.js, featuring glass morphism UI elements and dynamic light refraction effects",
  "keywords": [
    "react",
    "threejs",
    "3d",
    "prism",
    "visualization",
    "glass-morphism",
    "webgl",
    "interactive",
    "light-refraction"
  ],
  "author": "Parth Chandak",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/parthchandak02/prism-site.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "private": true,
  "dependencies": {
    "@react-three/drei": "10.0.7",
    "@react-three/fiber": "9.1.2",
    "@react-three/postprocessing": "3.0.4",
    "@types/three": "0.176.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1",
    "three": "0.176.0",
    "typeit": "^8.8.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "gh-pages": "^6.3.0",
    "kill-port": "^2.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.8"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false BROWSER=none react-scripts start",
    "dev": "npm run start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css}\"",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "health": "npm outdated && npm audit",
    "outdated": "npm outdated",
    "clean": "rimraf build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
